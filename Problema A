#include <iostream>

using namespace std;

int main() {
	int nrojuegos, cmtamano, cmjuegos, puedojugar;
	puedojugar = 0;
	cin >> nrojuegos;
	cin >> cmtamano;
	for (int i = 0; i < nrojuegos; i++) {
		cin >> cmjuegos;
		if (cmtamano >= cmjuegos) {
			puedojugar++;
		}
	}
	cout << puedojugar;
	return 0;
}
