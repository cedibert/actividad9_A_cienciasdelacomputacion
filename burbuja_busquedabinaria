#include <iostream>
using namespace std;
int main() {
	int n, temporal;
	int lista[100]; //el maximo de dimension a registrar
	cout << "Cuantos datos desea registrar?" << endl;
	cin >> n; // el maximo que se pone el usuario a registrar
	for (int i = 0; i < n; i++) {
		cin >> lista[i];
	}
	for (int i = 0; i < n; i++) {
		for (int j = i+1; i < n; j++) {
			if (lista[i] < lista[j]) {
				temporal = lista[j];
				lista[j] = lista[i];
				lista[i] = temporal;
			}
		}
	}

	for (int i = 0; i < n; i++) {
		cout << lista[i] << endl;
	}

	return 0;
}
